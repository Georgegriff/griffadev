{%- if github and github.url -%}
            <a title="View project on Github"  href="{{github.url}}" target="_blank">
                {%- include "img/github.svg" -%}
            </a>
            {%- endif -%}
            {%- if github and github.stars -%}
            <span aria-label="stars on Github">
                {%- include "img/star.svg" -%}
                <span>{{github.stars}} stars</span>
            </span>
            {%- endif -%}